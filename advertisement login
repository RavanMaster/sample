login.html
<html>
<head>
    <title>Login - ZenZone</title>
    <style>
        body { background: url('https://img.freepik.com/free-photo/blurred-clothing-store-shopping-mall_1258-5.jpg') no-repeat center/cover; font-family: Arial, sans-serif; text-align: center; }
        .log { margin-top: 15%; background: rgba(0, 0, 0, 0.7); padding: 30px; display: inline-block; border-radius: 10px; color: white; }
        input { width: 80%; padding: 10px; margin: 10px 0; border: none; border-radius: 5px; }
        .btn { background: #ff5733; color: white; padding: 10px 20px; border: none; cursor: pointer; border-radius: 5px; }
    </style>
</head>
<body>
    <div class="log">
        <h2>LOGIN</h2>
        <form onsubmit="return validateForm()">
            <input type="text" id="uname" placeholder="Username"><br>
            <input type="password" id="password" placeholder="Password"><br>
            <button type="submit" class="btn">Submit</button>
        </form>
    </div>
    <script>
        function validateForm() {
            let user = document.getElementById("uname").value;
            let pass = document.getElementById("password").value;
            if (user && pass) { window.location.href = "index.html"; return false; }
            alert("Please fill in both fields.");
            return false;
        }
    </script>
</body>
</html>





